@0000 A100 // LLB R1, 0x00
@0001 B102 // LHB R1, 0x02		# R1=0x0002
@0002 B201 // LLB R2, 0x01
@0003 A200 // LHB R2, 0x00		# R2=0x0001
@0004 B300 // LLB R3, 0x00
@0005 A300 // LHB R3, 0x00		# R3=0x0000
@0006 1112 // Repeat: SUB R1, R1, R2
@0007 C4FD // B   NEQ, Repeat
@0008 0332 // ADD R3, R3, R2		# R3=R3+0x0001